    "@tanstack/router-plugin": "1.58.4",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.1",
    "npm-run-all2": "6.2.3",
    "sort-package-json": "2.10.1",
    "vite": "5.4.7"
